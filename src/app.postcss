/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

.section-img {
	height: 100vh;
	width: 100vw;
	background-position: var(--bg-pos);
	background: linear-gradient(to top, #3204fdba, #9907facc), var(--url) no-repeat var(--bg-pos);
	background-repeat: no-repeat;
	background-size: cover;
}
